        $(".save_btn").click(function(){
            var formStructure = "{";
            var k = 0;
            
            $("#container_form ul#form_sortable li").each(function(){
                var id = "";
                var name = "";
                var label = "";
                var placeholder = "";
                var element_class = "";
                var rows = "";
                var cols = "";
                var description = "";
                var options = "";
                var element_value = "";
                    
                var elementType = $("> div:first span", this).html();
                elementType = elementType.replace("Element : ", "");
                
                switch (elementType) {
                    case "Textbox":
                    case "Password":
                    case "Uploadfield":
                        if(elementType === "Textbox") elementType = "textbox";
                        if(elementType === "Password") elementType = "password";
                        if(elementType === "Uploadfield") elementType = "uploadfield";
                        id = $("> div:last-child div.controls input", this).attr("id");
                        name = $("> div:last-child div.controls input", this).attr("name");
                        placeholder = $("> div:last-child div.controls input", this).attr("placeholder");
                        label = $("> div:last-child label", this).html();
                        element_class = $("> div:last-child div.controls input", this).attr("class");
                        
                        formStructure += "'" + k + "':{'" + elementType + "':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "','placeholder':'" + placeholder + "','label':'" + label + "'}},";
                        break;
                        
                    case "Textarea":
                        id = $("> div:last-child div.controls textarea", this).attr("id");
                        name = $("> div:last-child div.controls textarea", this).attr("name");
                        rows = $("> div:last-child div.controls textarea", this).attr("rows");
                        cols = $("> div:last-child div.controls textarea", this).attr("cols");
                        element_class = $("> div:last-child div.controls textarea", this).attr("class");
                        label = $("> div:last-child label", this).html();
                        
                        formStructure += "'" + k + "':{'textarea':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "','label':'" + label + "','rows':'" + rows + "','cols':'" + cols + "'}},";
                        break;
                        
                    case "Hidden":
                        id = $("> div:last-child div.controls input", this).attr("id");
                        name = $("> div:last-child div.controls input", this).attr("name");
                        element_class = $("> div:last-child div.controls input", this).attr("class");
                        
                        formStructure += "'" + k + "':{'hidden':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "'}},";
                        break;
                        
                    case "Checkbox":
                        id = $("> div:last-child div.controls input", this).attr("id");
                        name = $("> div:last-child div.controls input", this).attr("name");
                        element_class = $("> div:last-child div.controls input", this).attr("class");
                        description = $("> div:last-child div.controls span", this).html();
                        label = $("> div:last-child label", this).html();
                        
                        formStructure += "'" + k + "':{'checkbox':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "','description':'" + description + "','value':'1','label':'" + label + "'}},";
                        break;
                        
                    case "Selectbox":
                        id = $("> div:last-child div.controls select", this).attr("id");
                        name = $("> div:last-child div.controls select", this).attr("name");
                        element_class = $("> div:last-child div.controls select", this).attr("class");
                        label = $("> div:last-child label", this).html();
                        
                        $("> div:last-child div.controls select option", this).each(function(){
                            options += "'" + $(this).html() + "':'" + $(this).attr("value") + "',";
                        });
                        
                        options = options.substr(0, options.length - 1);
                        
                        formStructure += "'" + k + "':{'selectbox':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "','label':'" + label + "','options':{" + options + "}}},";
                        break;
                        
                    case "Radiobutton":
                        id = $("> div:last-child input", this).attr("id");
                        name = $("> div:last-child input", this).attr("name");
                        element_class = $("> div:last-child input", this).attr("class");
                        label = $("> div:last-child label", this).html();
                        
                        $("> div:last-child div.controls div", this).each(function(){
                            options += "'" + $("> span",this).html() + "':'" + $("> input",this).attr("value") + "',";
                        });
                        
                        options = options.substr(0, options.length - 1);
                        
                        formStructure += "'" + k + "':{'radiobutton':{'id':'" + id + "','class':'" + element_class + "','name':'" + name + "','label':'" + label + "','options':{" + options + "}}},";
                        break;
                        
                    case "Submit":
                    case "Button":
                        var btnType = "";
                        if(elementType === "Submit"){
                            btnType = "submit";
                        } else {
                            btnType = "button";
                        }
                        
                        id = $("> div:last-child input", this).attr("id");
                        element_class = $("> div:last-child input", this).attr("class");
                        element_value = $("> div:last-child div.controls input", this).val();
                        
                        formStructure += "'" + k + "':{'" + btnType + "':{'id':'" + id + "','class':'" + element_class + "','value':'" + element_value + "'}},";
                        break;
                }
                k++;
            });
            
            formStructure = formStructure.substr(0, formStructure.length - 1);
            formStructure += "}";
            $("#container_formdata form#saveFormData input#formStructure").val(formStructure);
            $("#container_formdata form#saveFormData").submit();
        });
        
        
